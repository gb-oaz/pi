{
  "name": "shell_core",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:build": "vue-tsc -b && vite build --mode development",
    "prod:build": "vue-tsc -b && vite build --mode production",
    "dev:start": "concurrently \"vite build --watch\" \"vite preview --port 3000 --strictPort\"",
    "prod:start": "concurrently \"vite build --watch\" \"vite preview --port 3000 --strictPort --host 0.0.0.0\""
  },
  "dependencies": {
    "@quasar/extras": "^1.16.17",
    "concurrently": "^8.2.2",
    "quasar": "^2.18.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@originjs/vite-plugin-federation": "^1.4.0",
    "@quasar/vite-plugin": "^1.9.0",
    "@types/node": "^22.13.14",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "sass-embedded": "^1.80.2",
    "typescript": "~5.7.2",
    "vite": "^6.2.6",
    "vue-tsc": "^2.2.4"
  }
}
